block js

doctype html
html
	head
		include head.pug
	body(class=darkmode && "darkmode")
		include menu-drop.pug
		
		.container-fluid
			include navbar.pug

			if modalType
				.modal-wrapper#modals
					block modals

			#page-content(class=noTop && "no-top")
				block content

			include footer.pug